<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/chistory"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GatekeeperHome">

    <TextView
        android:id="@+id/cmeratext"
        android:layout_width="178dp"
        android:layout_height="73dp"
        android:paddingLeft="30dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:text="Details"
        android:textAllCaps="true"
        android:textColor="@color/colorBlack"
        android:textColorHighlight="@color/colorBlack"
        android:textColorHint="@color/colorBlack"
        android:textColorLink="@color/colorBlack"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.231"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/cemailadressdlabel"
        android:layout_width="350dp"
        android:layout_height="49dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/inpuut_bg"
        android:drawableLeft="@drawable/ic_baseline_email_24"
        android:drawablePadding="5dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:text="Email Address:"
        android:textColor="@color/colorBlack"
        android:textColorHighlight="@color/colorBlack"
        android:textColorHint="@color/colorBlack"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/cnameLabel2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/cnameLabel2"
        app:layout_constraintTop_toBottomOf="@+id/cnameLabel2" />

    <TextView
        android:id="@+id/cnameLabel2"
        android:layout_width="350dp"
        android:layout_height="49dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/inpuut_bg"
        android:drawableLeft="@drawable/ic_face"
        android:drawablePadding="5dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:text="Name:"
        android:textColor="@color/colorBlack"
        android:textColorHighlight="@color/colorBlack"
        android:textColorHint="@color/colorBlack"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/cmeratext"
        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintStart_toStartOf="@+id/cmeratext"
        app:layout_constraintTop_toBottomOf="@+id/cmeratext" />

    <TextView
        android:id="@+id/cphonenumberLabel2"
        android:layout_width="350dp"
        android:layout_height="49dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/inpuut_bg"
        android:drawableLeft="@drawable/ic_baseline_contact_phone_24"
        android:drawablePadding="5dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:text="PhoneNumber:"
        android:textColor="@color/colorBlack"
        android:textColorHighlight="@color/colorBlack"
        android:textColorHint="@color/colorBlack"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/caddress"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/caddress"
        app:layout_constraintTop_toBottomOf="@+id/caddress" />

    <TextView
        android:id="@+id/caddress"
        android:layout_width="350dp"
        android:layout_height="49dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/inpuut_bg"
        android:drawableLeft="@drawable/ic_baseline_apartment_24"
        android:drawablePadding="5dp"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:text="Address:"
        android:textColor="@color/colorBlack"
        android:textColorHighlight="@color/colorBlack"
        android:textColorHint="@color/colorBlack"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/cemailadressdlabel"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/cemailadressdlabel"
        app:layout_constraintTop_toBottomOf="@+id/cemailadressdlabel" />

    <Button
        android:id="@+id/chistories"
        android:layout_width="165dp"
        android:layout_height="43dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/inpuut_bg"
        android:text="HISTORY"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="@color/design_default_color_primary_dark"
        app:layout_constraintEnd_toEndOf="@+id/cphonenumberLabel2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/cphonenumberLabel2"
        app:layout_constraintTop_toBottomOf="@+id/cphonenumberLabel2" />

    <Button
        android:id="@+id/crequests"
        android:layout_width="167dp"
        android:layout_height="43dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/inpuut_bg"
        android:text="LogOut"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="@color/design_default_color_primary_dark"
        app:layout_constraintEnd_toEndOf="@+id/cphonenumberLabel2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/cphonenumberLabel2"
        app:layout_constraintTop_toBottomOf="@+id/cphonenumberLabel2" />

    <Button
        android:id="@+id/cviewaccepted"
        android:layout_width="229dp"
        android:layout_height="39dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/inpuut_bg"
        android:text="View Accepted Request"
        app:backgroundTint="@color/design_default_color_primary_variant"
        app:layout_constraintEnd_toEndOf="@+id/chistories"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="@+id/crequests"
        app:layout_constraintTop_toBottomOf="@+id/crequests" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="107dp"
        android:layout_height="126dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047"
        app:srcCompat="@drawable/jgiicon" />

</androidx.constraintlayout.widget.ConstraintLayout>