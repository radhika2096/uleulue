<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".matapitaprofileView">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="401dp"
        android:layout_height="400dp"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <EditText
                android:id="@+id/exitDate2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginBottom="32dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_menu_month"
                android:ems="10"
                android:hint="Enter Exit Date"
                android:inputType="date"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#090909"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/exitTime2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/exitTime2" />

            <EditText
                android:id="@+id/exitTime2"
                android:layout_width="350dp"
                android:layout_height="47dp"

                android:layout_marginBottom="16dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@drawable/ic_time"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint=" Enter exit time"
                android:inputType="time"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#0E0E0E"
                app:layout_constraintBottom_toTopOf="@+id/exitDate2"
                app:layout_constraintEnd_toEndOf="@+id/entryDate2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/entryDate2" />

            <EditText
                android:id="@+id/entryTime2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_lock_idle_alarm"
                android:ems="10"
                android:hint=" Enter Entry Time"
                android:inputType="time"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#151515"
                app:layout_constraintBottom_toTopOf="@+id/entryDate2"
                app:layout_constraintEnd_toEndOf="@+id/adress2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/adress2"
                app:layout_constraintTop_toBottomOf="@+id/adress2" />

            <EditText
                android:id="@+id/adress2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_dialog_map"
                android:ems="10"
                android:hint=" Address "
                android:inputType="textPostalAddress"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#0C0C0C"
                app:layout_constraintEnd_toEndOf="@+id/textView3"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView3"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <EditText
                android:id="@+id/entryDate2"
                android:layout_width="350dp"
                android:layout_height="47dp"
                android:layout_marginBottom="32dp"
                android:background="@drawable/inpuut_bg"
                android:drawableLeft="@android:drawable/ic_menu_month"
                android:ems="10"
                android:hint="Enter Entry date"
                android:inputType="date"
                android:padding="10dp"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColorHint="#101010"
                app:layout_constraintBottom_toTopOf="@+id/exitTime2"
                app:layout_constraintEnd_toEndOf="@+id/entryTime2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/entryTime2" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:text="     Student OutPass"
                android:textAllCaps="false"
                android:textColor="#3F51B5"
                android:textSize="34sp"
                android:textStyle="bold|italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/usernameparentsshow"
        android:layout_width="290dp"
        android:layout_height="59dp"
        android:text="Username"
        android:textColor="#070707"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/showPhone"
        android:layout_width="127dp"
        android:layout_height="33dp"
        android:layout_marginTop="9dp"
        android:text="proffession"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/showEmail"
        app:layout_constraintEnd_toEndOf="@+id/usernameparentsshow"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/usernameparentsshow"
        app:layout_constraintTop_toBottomOf="@+id/usernameparentsshow"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/request9810"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:text="request"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cancelRequest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView" />

    <Button
        android:id="@+id/cancelRequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:text="cancel Request"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView" />

    <TextView
        android:id="@+id/showEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="ShowEmail"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="@+id/showPhone"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/showPhone" />

</androidx.constraintlayout.widget.ConstraintLayout>