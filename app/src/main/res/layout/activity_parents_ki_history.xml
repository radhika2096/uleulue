<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundd"
    tools:context=".parentsKiHistory">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

    <TextView
        android:id="@+id/textView32"
        android:layout_width="238dp"
        android:layout_height="45dp"
        android:gravity="center"
        android:text="History"
        android:layout_gravity="center"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"
        />

    <ImageView
        android:id="@+id/imageViewp"
        android:layout_width="120dp"
        android:layout_gravity="right"
        android:layout_height="95dp"

        app:srcCompat="@drawable/jgiicon" />

        </LinearLayout>

    <ListView
        android:id="@+id/lv"
        android:layout_width="412dp"
        android:layout_height="458dp"
        android:background="#B048E310"
        android:cacheColorHint="#F42F2F"
        android:listSelector="@color/design_default_color_primary_dark"
      >

    </ListView>
    </LinearLayout>


</RelativeLayout>